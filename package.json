{
  "name": "whatnow",
  "version": "1.0.0",
  "description": "WhatNow is a web-based app built on the MERN stack",
  "main": "app.js",
  "scripts": {
    "server:debug": "nodemon --inspect app.js",
    "start": "node app.js",
    "server": "nodemon app.js",
    "frontend-install": "npm install --prefix frontend",
    "frontend": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run frontend\"",
    "dev:debug": "concurrently \"npm run server:debug\" \"npm run frontend\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stephl3/WhatNow.git"
  },
  "author": "Christopher Trinh, Nathaniel Kevin Suriawijaya, Lisa Wen, Stephen Lee",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stephl3/WhatNow/issues"
  },
  "homepage": "https://github.com/stephl3/WhatNow#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@primer/octicons-react": "^9.3.0",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "frontend": "file:frontend",
    "google-maps-react": "^2.0.2",
    "googleapis": "^45.0.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "mongoose": "^5.7.11",
    "node-sass": "^7.0.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react-icons": "^3.8.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "validator": "^12.0.0"
  },
  "devDependencies": {
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "dotenv": "^8.2.0",
    "nodemon": "^1.19.4"
  }
}
