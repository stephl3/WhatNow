{
  "name": "whatnow",
  "version": "1.0.0",
  "description": "WhatNow is a web-based app built on the MERN stack",
  "main": "app.js",
  "scripts": {
    "server:debug": "nodemon --inspect app.js",
    "start": "node app.js",
    "server": "nodemon app.js",
    "frontend-install": "npm install --prefix frontend",
    "frontend": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run frontend\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stephl3/WhatNow.git"
  },
  "author": "Christopher Trinh, Nathaniel Kevin Suriawijaya, Lisa Wen, Stephen Lee",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stephl3/WhatNow/issues"
  },
  "homepage": "https://github.com/stephl3/WhatNow#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.11",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^12.0.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.4"
  }
}
